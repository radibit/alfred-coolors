    "color-namer": "^1.1.0"
  },
  "devDependencies": {
    "alfy-test": "^0.4.0",
    "ava": "^0.15.2",
    "xo": "^0.36.1"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 3,
          "maxEOF": 1
        }
      ]
    }
