{
  "name": "alfred-coolors",
  "version": "0.3.0",
  "description": "Get color name",
  "license": "MIT",
  "repository": "radibit/alfred-coolors",
  "author": "Radimir Bitsov <radimir.bitsov@gmail.com>",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
